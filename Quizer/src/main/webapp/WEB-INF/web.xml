    <servlet-name>QuizerValidation</servlet-name>
    <servlet-class>com.quizer.connection.MYSQLConnection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuizerValidation</servlet-name>
    <url-pattern>/quizerURL</url-pattern>
  </servlet-mapping>
