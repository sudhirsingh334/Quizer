  </servlet>
  <servlet-mapping>
    <servlet-name>AddQuestionServlet</servlet-name>
    <url-pattern>/pages/addquestion</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>QuizManagerServlet</display-name>
    <servlet-name>QuizManagerServlet</servlet-name>
    <servlet-class>com.quizer.servlet.QuizManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuizManagerServlet</servlet-name>
    <url-pattern>/pages/quizmanager</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PrepareQuizServlet</display-name>
    <servlet-name>PrepareQuizServlet</servlet-name>
    <servlet-class>com.quizer.servlet.PrepareQuizServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PrepareQuizServlet</servlet-name>
    <url-pattern>/pages/PrepareQuizServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StartQuizServlet</display-name>
